{
  "version": 1,
  "canisters": {
    "blocktrace_backend": {
      "type": "rust",
      "package": "blocktrace-dapp-main-backend",
      "candid": "src/blocktrace-dapp-main-backend/blocktrace-dapp-main-backend.did"
    },
    "nft_backend": {
      "type": "rust", 
      "package": "nft-backend",
      "candid": "src/nft-backend/nft-backend.did"
    },
    "blocktrace_frontend": {
      "type": "assets",
      "source": ["src/blocktrace-frontend-main/out/"]
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8081",
      "type": "ephemeral",
      "replica": {
        "subnet_type": "system"
      }
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "output_env_file": ".env",
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  }
}